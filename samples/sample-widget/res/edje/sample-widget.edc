collections {
	group {
		name: "main";
		parts {
			part {
				name: "bg";
				type: RECT;
				description {
					state: "default" 0.0;
					color: 100 100 100 255;
				}
			}
			part {
				name: "txt_title";
				type: TEXT;
				mouse_events: 0;
				description {
					state: "default" 0.0;
					align: 0.5 0.5;
					text {
						font: "Tizen:style=regular";
						size: 40;
						min: 1 1;
						ellipsis: -1;
					}
					color: 255 255 255 255;
				}
			}
		}
	}
}
